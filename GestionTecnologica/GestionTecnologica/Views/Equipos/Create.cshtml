@model GestionTecnologica.Models.Equipo

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>Equipo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NumeroSerie" class="control-label"></label>
                <input asp-for="NumeroSerie" class="form-control" />
                <span asp-validation-for="NumeroSerie" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdTipoEquipo" class="control-label"></label>
                <select asp-for="IdTipoEquipo" class="form-control" asp-items="ViewBag.IdTipoEquipo">
                    <option>
                        Seleccionar
                    </option>
                </select>
                <span asp-validation-for="IdTipoEquipo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdSede" class="control-label"></label>
                <select asp-for="IdSede" class="form-control" asp-items="ViewBag.IdSede">
                    <option>
                        Seleccionar
                    </option>
                </select>
                <span asp-validation-for="IdSede" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaCompra" class="control-label"></label>
                <input asp-for="FechaCompra" class="form-control" />
                <span asp-validation-for="FechaCompra" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaExpiracionGarantia" class="control-label"></label>
                <input asp-for="FechaExpiracionGarantia" class="form-control" />
                <span asp-validation-for="FechaExpiracionGarantia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
